function resetSlick(slick){const vw=Math.max(document.documentElement.clientWidth||0,window.innerWidth||0);console.log(slick);var enabled=$("#"+slick.id+"-slick").val();if(vw<766||enabled){$(slick).slick("setPosition");}}
function slickCards(id,slidesToShow,slidesToScroll,enabled){if(id.substring(0,1)!='#'){id="#"+id;}
var bLazy=new Blazy();if(enabled){$(id).slick({dots:true,slidesToShow:slidesToShow,slidesToScroll:slidesToScroll,infinite:true,arrows:true,draggable:false,prevArrow:'<button type="button" class="slick-prev"><img src="/Images/chevron-left-circle.svg" alt="Left arrow" aria-label="Carousel Prev Items"></button>',nextArrow:'<button type="button" class="slick-next"><img src="/Images/chevron-right-circle.svg" alt="Right arrow" aria-label="Carousel Next Items"></button>',responsive:[{breakpoint:767,settings:{arrows:true,dots:true,slidesToShow:1,slidesToScroll:1,draggable:false},}]});}
else{const vw=Math.max(document.documentElement.clientWidth||0,window.innerWidth||0);$(id).slick({responsive:[{breakpoint:4000,settings:'unslick'},{breakpoint:767,settings:{arrows:true,dots:true,slidesToShow:1,slidesToScroll:1,draggable:false,prevArrow:'<button type="button" class="slick-prev"><img src="/Images/chevron-left-circle.svg" alt="Left arrow" aria-label="Carousel Prev Items"></button>',nextArrow:'<button type="button" class="slick-next"><img src="/Images/chevron-right-circle.svg" alt="Right arrow" aria-label="Carousel Next Items"></button>',},}]});}
$(id).on('afterChange',function(){bLazy.revalidate();});};;
